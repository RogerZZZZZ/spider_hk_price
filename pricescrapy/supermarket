<script type="text/javascript">
	(function ($) {
		$( document ).ready(function() {
			$('a.menuparent').hover(function() {
				var target_menu = $(this).parent();
				$('ul.sf-menu li.menuparent').removeClass('sfHover');
				$('ul.sf-menu li.menuparent').find('ul').addClass('sf-hidden');
				target_menu.addClass('sfHover');
				target_menu.find('ul').removeClass('sf-hidden');
			});
			$('a.menuparent').focus(function() {
				var target_menu = $(this).parent();
				$('ul.sf-menu li.menuparent').removeClass('sfHover');
				$('ul.sf-menu li.menuparent').find('ul').addClass('sf-hidden');
				target_menu.addClass('sfHover');
				target_menu.find('ul').removeClass('sf-hidden');
			});
			$('.main-container').hover(function() {
				$('ul.sf-menu li.menuparent').removeClass('sfHover');
				$('ul.sf-menu li.menuparent').find('ul').addClass('sf-hidden');
			});
			$('.main-container').focus(function() {
				$('ul.sf-menu li.menuparent').removeClass('sfHover');
				$('ul.sf-menu li.menuparent').find('ul').addClass('sf-hidden');
			});
		});
	})(jQuery);	
</script>

